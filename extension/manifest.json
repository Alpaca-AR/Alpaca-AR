{
  "name": "Alpaca",
  "version": "0.1.0",
  "manifest_version": 2,
  "description": "Mixed Reality",
  "web_accessible_resources": ["three.js", "alpaca.js"],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "content_scripts": [
    {
      "js": ["three.js", "alpaca.js", "nps.js"],
      "matches": ["https://science.nature.nps.gov/parks/grsm/species/"]
    },
    {
      "js": ["three.js", "alpaca.js", "google-books.js"],
      "matches": ["https://books.google.com/*"]
    }
  ],
  "browser_action": {
    "default_title": "Alpaca",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "https://*/*",
    "http://*/*",
    "tabs",
    "unlimitedStorage"
  ]
}
